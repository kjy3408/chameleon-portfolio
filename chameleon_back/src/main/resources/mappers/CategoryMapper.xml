<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chameleon.band.repository.CategoryRepository">
	
	<select id="getCategories" resultType="com.chameleon.band.dto.response.CategoryResponseDto">
		SELECT
			category_id as categoryId,
			category_name as categoryName
		FROM
			category_tb
	</select>
</mapper>